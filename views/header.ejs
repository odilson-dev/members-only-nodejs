<header>
  <h1>Members Only</h1>
  <div class="account-status-box">
    <% if(currentUser){ %>
    <div class="account-status">
      <% if (currentUser.admin){ %> <%= "Admin" %> <% } else if
      (currentUser.membership_status){ %> <%= "member" %> <% } else{ %> <%=
      "Basic" %> <% } %> Account
    </div>

    <% } %>
  </div>
</header>
